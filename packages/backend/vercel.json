{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/update-all.ts": {
      "includeFiles": "lib/**/*.ts",
      "maxDuration": 60
    },
    "api/[space]/latest/update.ts": {
      "includeFiles": "lib/**/*.ts",
      "maxDuration": 60
    },
    "api/[space]/snapshot/[blocknumber]/strategy-formatted-vote-weights": {
      "includeFiles": "lib/**/*.ts",
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/update-all",
      "schedule": "0 */6 * * *"
    }
  ]
}
